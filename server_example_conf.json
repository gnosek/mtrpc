{
    "rpc_tree_init": {
        "imports": ["mtrpc.server.sysmethods as system"],
        "paths": [
            "server_example_methods.py as example",
            "netservice_methods.py as netservices"
        ],
        "mod_init_kwargs": {
            "logging_settings": {
                "handlers": [
                    {
                        "cls": "StreamHandler", 
                        "format": "%(asctime)s %(name)s:%(levelname)s @%(threadName)s: %(message)s", 
                        "kwargs": {}, 
                        "level": "info"
                    },
                    {
                        "cls": "RotatingFileHandler", 
                        "format": "%(asctime)s %(name)s:%(levelname)s @%(threadName)s: %(message)s", 
                        "kwargs": {
                            "filename": "/var/log/mtrpc/example.log",
                            "maxBytes": 10485760,
                            "backupCount": 5,
                            "encoding": "utf8"
                        },
                        "level": "info"
                    }
                ],
                "level": "warning",
                "mod_logger_pattern": "mtrpc.server.rpc_log.{full_name}",
                "propagate": false
            },
            "mod_globals": {
                "netservices": {
                    "MAILDROP_CONF_PATH_PATTERN": "/home/{account}/mail/{domain}/{localpart}/.mailfilter"
                }
            }
        }
    }, 
    "amqp_params": {
        "host": "localhost:5672",
        "userid": "guest",
        "password": "guest",
        "virtual_host": "/"
    }, 
    "bindings": [
        ["rpc.megiteam.example", "#", "", ""]
    ], 
    "exchange_types": {
        "rpc.megiteam.example": "topic"
    }, 
    "manager_settings": {
        "client_id": "rpc.megiteam.example"
    },
    "manager_attributes": {}, 
    "responder_attributes": {},
    "logging_settings": {
        "handlers": [
            {
                "cls": "StreamHandler", 
                "format": "%(asctime)s %(name)s:%(levelname)s @%(threadName)s: %(message)s", 
                "kwargs": {}, 
                "level": "info"
            },
            {
                "cls": "RotatingFileHandler", 
                "format": "%(asctime)s %(name)s:%(levelname)s @%(threadName)s: %(message)s", 
                "kwargs": {
                    "filename": "/var/log/mtrpc/example.log",
                    "maxBytes": 10485760,
                    "backupCount": 5,
                    "encoding": "utf8"
                },
                "level": "info"
            }
        ], 
        "level": "info", 
        "server_logger": "mtrpc.server",
        "propagate": false
    }, 
    "os_settings": {
        "daemon": false, 
        "signal_actions": {"SIGHUP": "restart", "SIGTERM": "exit", "SIGINT": "exit"}, 
        "stopping_timeout": 10, 
        "umask": null, 
        "working_dir": null
    }
}
